@model Room




@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h2>@Model.RoomName</h2>
<h3>Patients</h3>
    <table class="table">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var pat in Model.Patients)
            {
                <tr>
                    <td>@pat.FirstName</td>
                    <td>@pat.LastName</td>
                </tr>
            }
        </tbody>
    </table>
