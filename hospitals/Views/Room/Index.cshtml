@model IEnumerable<Room>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>
    Rooms
</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Room Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var room in Model)
            {
                <tr>
                    <td>
                        <a asp-controller="Room" asp-action="Detail" asp-route-id="@room.RoomId"> @room.RoomName</a> 
                    </td>
                </tr>
            }
        </tbody>
    </table>


